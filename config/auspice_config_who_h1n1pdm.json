  "color_options": {
    "gt": {
      "menuItem": "genotype",
      "legendTitle": "Genotype",
      "type": "discrete",
      "key": "genotype"
    },
    "num_date": {
      "menuItem": "date",
      "legendTitle": "Sampling date",
      "type": "continuous",
      "key": "num_date"
    },
    "region": {
      "key":"region",
      "legendTitle":"Region",
      "menuItem":"region",
      "type":"discrete"
    },
    "clade_membership": {
      "key": "clade_membership",
      "legendTitle": "Clade",
      "menuItem": "clade",
      "type": "discrete"
    },
    "avgage": {
      "menuItem": "average age",
      "legendTitle": "average age",
      "type": "continuous",
      "key": "avg_age"
    },
    "vaccov": {
      "key":"vac_cov",
      "legendTitle":"Host vaccine coverage",
      "menuItem":"Host  vac. cov.",
      "type":"continuous"
    },
    "cTiter": {
      "key": "cTiter",
      "legendTitle": "Antigenic advance (tree model)",
      "menuItem": "antigenic advance (tree model)",
      "type": "continuous"
    },
    "cTiterSub": {
      "key": "cTiterSub",
      "legendTitle": "Antigenic advance (sub model)",
      "menuItem": "antigenic advance (sub model)",
      "type": "continuous"
    },
    "lbi": {
      "vmin": 0,
      "vmax": 0.7,
      "legendTitle": "local branching index",
      "key": "lbi",
      "menuItem": "local branching index",
      "type": "continuous"
    },
    "ep": {
      "menuItem": "epitope mutations",
      "legendTitle": "Epitope mutations",
      "type": "continuous",
      "key": "ep"
    },
    "ne": {
      "menuItem": "non-epitope mutations",
      "legendTitle": "Non-epitope mutations",
      "type": "continuous",
      "key": "ne"
    }
  },
  "geo": [
    "country",
    "region"
  ],
  "defaults": {
    "mapTriplicate": true,
    "colorBy": "clade_membership"
  },
  "filters": [
    "clade_membership",
    "region",
    "country",
    "authors"
