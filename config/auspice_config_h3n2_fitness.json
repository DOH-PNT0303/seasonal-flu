{
  "title": "Real-time tracking of influenza A/H3N2 evolution",
  "colorings": {
    "gt": {
      "title": "Genotype",
      "type": "categorical"
    },
    "num_date": {
      "title": "Date",
      "type": "continuous"
    },
    "clade_membership": {
      "title": "Clade",
      "type": "categorical"
    },
    "region": {
      "title": "Region",
      "type": "categorical"
    },
    "country": {
      "title": "Country",
      "type": "categorical"
    },
    "cTiter": {
      "title": "Antigenic advance (tree model)",
      "type": "continuous"
    },
    "cTiterSub": {
      "title": "Antigenic advance (sub model)",
      "type": "continuous"
    },
    "lbi": {
      "vmin": 0,
      "vmax": 0.7,
      "title": "Local branching index",
      "type": "continuous"
    },
    "ep": {
      "title": "Epitope mutations",
      "type": "ordinal"
    },
    "ne": {
      "title": "Non-epitope mutations",
      "type": "ordinal"
    },
    "rb": {
      "title": "RBS adjacent mutations",
      "type": "ordinal"
    },
    "fitness": {
      "title": "Fitness",
      "type": "continuous"
    },
    "weighted_distance_to_future": {
      "title": "Distance to future population",
      "type": "continuous"
    },
    "submitting_lab": {
      "title": "Submitting lab",
      "type": "categorical"
    },
    "originating_lab": {
      "title": "Originating lab",
      "type": "categorical"
    }
  },
  "geo_resolutions": [
    "country",
    "region"
  ],
  "display_defaults": {
    "map_triplicate": true,
    "color_by": "clade_membership"
  },
  "maintainers": [
    ["Jover Lee", "https://bedford.io/team/jover-lee/"],
    ["Richard Neher", "https://neherlab.org/richard-neher.html"],
    ["Trevor Bedford", "https://bedford.io/team/trevor-bedford/"]
  ],
  "filters": [
    "clade_membership",
    "region",
    "country",
    "submitting_lab"
  ],
  "panels": [
    "tree",
    "map",
    "entropy",
    "frequencies"
  ],
  "vaccine_choices": {
    "A/Wisconsin/67/2005": "2006-02-21",
    "A/Brisbane/10/2007": "2007-09-25",
    "A/Perth/16/2009": "2009-09-25",
    "A/Victoria/361/2011": "2012-02-21",
    "A/Texas/50/2012": "2013-09-25",
    "A/Switzerland/9715293/2013": "2014-09-25",
    "A/HongKong/4801/2014": "2015-09-24",
    "A/Singapore/Infimh-16-0019/2016": "2017-09-28",
    "A/Switzerland/8060/2017": "2018-09-27",
    "A/Kansas/14/2017": "2019-03-21",
    "A/SouthAustralia/34/2019": "2019-09-27"
  }
}
