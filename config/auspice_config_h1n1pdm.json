{
  "title": "Real-time tracking of influenza A/H1N1pdm evolution",
  "colorings": {
    "gt": {
      "title": "Genotype",
      "type": "categorical"
    },
    "num_date": {
      "title": "Date",
      "type": "continuous"
    },
    "clade_membership": {
      "title": "Clade",
      "type": "categorical"
    },
    "region": {
      "title": "Region",
      "type": "categorical"
    },
    "country": {
      "title": "Country",
      "type": "categorical"
    },
    "cTiter": {
      "title": "Antigenic advance (tree model)",
      "type": "continuous"
    },
    "cTiterSub": {
      "title": "Antigenic advance (sub model)",
      "type": "continuous"
    },
    "lbi": {
      "vmin": 0,
      "vmax": 0.7,
      "title": "Local branching index",
      "type": "continuous"
    },
    "ep": {
      "title": "Epitope mutations",
      "type": "ordinal"
    },
    "ne": {
      "title": "Non-epitope mutations",
      "type": "ordinal"
    },
    "submitting_lab": {
      "title": "Submitting lab",
      "type": "categorical"
    },
    "originating_lab": {
      "title": "Originating lab",
      "type": "categorical"
    }
  },
  "geo_resolutions": [
    "country",
    "region"
  ],
  "display_defaults": {
    "map_triplicate": true,
    "color_by": "clade_membership"
  },
  "maintainers": [
    ["Jover Lee", "https://bedford.io/team/jover-lee/"],
    ["Richard Neher", "https://neherlab.org/richard-neher.html"],
    ["Trevor Bedford", "https://bedford.io/team/trevor-bedford/"]
  ],
  "filters": [
    "clade_membership",
    "region",
    "country",
    "submitting_lab"
  ],
  "panels": [
    "tree",
    "map",
    "entropy",
    "frequencies"
  ],
  "vaccine_choices": {
    "A/California/7/2009": "2009-09-25",
    "A/Michigan/45/2015": "2016-09-29",
    "A/Brisbane/2/2018": "2019-02-20"
  }
}
