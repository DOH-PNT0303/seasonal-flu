{
  "title": "Real-time tracking of influenza B/Yam evolution using data from GISAID",
  "maintainers": [
    {"name": "Jover Lee", "url": "https://bedford.io/team/jover-lee/"},
    {"name": "Richard Neher", "url": "https://neherlab.org/richard-neher.html"},
    {"name": "Trevor Bedford", "url": "https://bedford.io/team/trevor-bedford/"}
  ],
  "color_options": {
    "gt": {
      "title": "genotype",
      "type": "categorical",
      "key": "genotype"
    },
    "num_date": {
      "title": "Sampling date",
      "type": "continuous",
      "key": "num_date"
    },
    "region": {
      "key":"region",
      "title":"Region",
      "type":"categorical"
    },
    "clade_membership": {
      "key": "clade_membership",
      "title": "Clade",
      "type": "categorical"
    },
    "cTiter": {
      "key": "cTiter",
      "title": "Antigenic advance (tree model)",
      "type": "continuous"
    },
    "cTiterSub": {
      "key": "cTiterSub",
      "title": "Antigenic advance (sub model)",
      "type": "continuous"
    },
    "lbi": {
      "vmin": 0,
      "vmax": 0.7,
      "key": "lbi",
      "title": "local branching index",
      "type": "continuous"
    }
  },
  "geo": [
    "country",
    "region"
  ],
  "defaults": {
    "mapTriplicate": true,
    "colorBy": "clade_membership"
  },
  "filters": [
    "clade_membership",
    "region",
    "country",
    "authors"
  ],
  "panels": [
    "tree",
    "map",
    "entropy",
    "frequencies"
  ]
}
