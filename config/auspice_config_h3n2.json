{
  "title": "Real-time tracking of influenza A/H3N2 evolution",
  "colorings": {
    "gt": {
      "title": "genotype",
      "type": "categorical",
      "key": "genotype"
    },
    "num_date": {
      "title": "Sampling date",
      "type": "continuous",
      "key": "num_date"
    },
    "region": {
      "key":"region",
      "title":"Region",
      "type":"categorical"
    },
    "clade_membership": {
      "key": "clade_membership",
      "title": "Clade",
      "type": "categorical"
    },
    "cTiter": {
      "key": "cTiter",
      "title": "Antigenic advance (tree model)",
      "type": "continuous"
    },
    "cTiterSub": {
      "key": "cTiterSub",
      "title": "Antigenic advance (sub model)",
      "type": "continuous"
    },
    "lbi": {
      "vmin": 0,
      "vmax": 0.7,
      "key": "lbi",
      "title": "local branching index",
      "type": "continuous"
    },
    "ep": {
      "title": "epitope mutations",
      "type": "ordinal",
      "key": "ep"
    },
    "ne": {
      "title": "non-epitope mutations",
      "type": "ordinal",
      "key": "ne"
    },
    "rb": {
      "title": "RBS adjacent mutations",
      "type": "ordinal",
      "key": "rb"
    }
  },
  "geo_resolutions": [
    {"key":"country"},
    {"key":"region"}
  ],
  "display_defaults": {
    "map_triplicate": true,
    "color_by": "clade_membership"
  },
  "maintainer": [
    "Trevor Bedford and Barney Potter",
    "http://bedford.io/"
  ],
  "filters": [
    "clade_membership",
    "region",
    "country",
    "authors"
  ],
  "panels": [
    "tree",
    "map",
    "entropy",
    "frequencies"
  ],
  "vaccine_choices": {
    "A/Wisconsin/67/2005": "2006-02-21",
    "A/Brisbane/10/2007": "2007-09-25",
    "A/Perth/16/2009": "2009-09-25",
    "A/Victoria/361/2011": "2012-02-21",
    "A/Texas/50/2012": "2013-09-25",
    "A/Switzerland/9715293/2013": "2014-09-25",
    "A/HongKong/4801/2014": "2015-09-24",
    "A/Singapore/Infimh-16-0019/2016": "2017-09-28",
    "A/Switzerland/8060/2017": "2018-09-27",
    "A/Kansas/14/2017": "2019-03-21"
  }
}
