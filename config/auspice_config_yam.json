      "type": "categorical"
    },
    "num_date": {
      "title": "Date",
      "type": "continuous"
    },
    "clade_membership": {
      "title": "Clade",
      "type": "categorical"
    },
    "region": {
      "title": "Region",
      "type": "categorical"
    },
    "country": {
      "title": "Country",
      "type": "categorical"
    },
    "submitting_lab": {
      "title": "Submitting lab",
      "type": "categorical"
    },
    "cTiter": {
      "title": "Antigenic advance (tree model)",
      "type": "continuous"
    },
    "cTiterSub": {
      "title": "Antigenic advance (sub model)",
      "type": "continuous"
    },
    "lbi": {
      "vmin": 0,
